{
  "[json][jsonc][typescript][typescriptreact][javascript][javascriptreact][html][css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "deno.enablePaths": [
    "supabase/functions"
  ],
  "deno.lint": true,
  "deno.unstable": [
    "bare-node-builtins",
    "byonm",
    "sloppy-imports",
    "unsafe-proto",
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "ffi",
    "fs",
    "http",
    "net"
  ],
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "app.config.*": "jsconfig*, vitest.config.*, cypress.config.*, playwright.config.*",
    "package.json": "package-lock.json, pnpm*, .yarnrc*, yarn*, .eslint*, eslint*, .prettier*, prettier*, .editorconfig, .gitattributes, bun.lock",
    "tsconfig.json": "tsconfig.*.json, env.d.ts"
  },
  "files.eol": "\n",
  "files.readonlyInclude": {
    "**/routeTree.gen.ts": true,
    "bun.lock": true,
    "pnpm-lock.yaml": true
  },
  "files.watcherExclude": {
    "**/routeTree.gen.ts": true,
    "bun.lock": true,
    "pnpm-lock.yaml": true
  },
  "search.exclude": {
    "**/routeTree.gen.ts": true,
    "bun.lock": true,
    "pnpm-lock.yaml": true
  },
  "typescript.tsdk": "./node_modules/typescript/lib"
}
